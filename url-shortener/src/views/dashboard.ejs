<!DOCTYPE html>
<html>
<head>
  <title>Dashboard</title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body>
  <div class="card">
    <h1>Your Shortened URLs</h1>
    <ul>
      <% urls.forEach(url => { %>
        <li>
          <strong><a href="/<%= url.shortId %>"><%= url.shortId %></a></strong><br>
          <span style="color:#555;">Original: <%= url.originalUrl %></span><br>
          <span>Clicks: <%= url.clicks %></span><br>
          <a href="/analytics/<%= url.shortId %>">Analytics</a>
        </li>
      <% }) %>
    </ul>
  </div>
</body>
</html>
